lista = ../lista/lista.0

{ comandos } => lista.mapear({
  lista: comandos
  função: comando =>
    | comando.tipo == "escreva" = [
      "console.log('"
      comando.texto
      "')"
    ] * ""
    | comando.tipo == "saia" = [
      "process.exit("
      "" + comando.código
      ")"
    ] * ""
    | "throw new Error('Comando desconhecido.')"
}) * ";"
