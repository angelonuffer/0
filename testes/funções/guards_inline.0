    5
  ])
  uniteste.iguais([
    (
      max = { a b } => | a > b = a | b
      max({ a: 10 b: 5 })
    )
    10
  ])
  uniteste.iguais([
    (
      max = { a b } => | a > b = a | b
      max({ a: 3 b: 8 })
    )
    8
  ])
]})